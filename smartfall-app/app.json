{
  "expo": {
    "name": "SmartFall",
    "slug": "smartfall",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "smartfall",
    "userInterfaceStyle": "automatic",
    "android": {
      "package": "com.smartfall.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartfall.app"
    },
    "extra": {
      "expoPublicApiUrl": "http://localhost:4000"
    }
  }
}

